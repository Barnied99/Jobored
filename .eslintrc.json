{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "next",
    "next/core-web-vitals"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": "warn",
    "import/prefer-default-export": "off",
    "react/jsx-no-useless-fragment": "warn",
    "no-unused-vars": "warn",
    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ],
        "newlines-between": "always"
      }
    ]
  }
}